{
  "name": "cadastro-colaborador",
  "version": "1.0.0",
  "description": "Cadastro Colaborador",
  "main": "index.js",
  "proxy": "http://localhost:4000/",
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.3",
    "eslint": "^4.16.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "2.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "2.42.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "^2.5.0",
    "eslint-plugin-react": "7.6.0",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.10.2"
  },
  "scripts": {
    "//": "=== INSTALL ===",
    "postinstall": "yarn run install:server",
    "install:server": "cd ./_server && yarn install",
    "//1": "=== START ===",
    "start": "yarn run start:server",
    "start:server": "cd ./_server && yarn start",
    "start:client": "npm-run-all -p watch-css start-js",
    "//2": "=== TEST ===",
    "test": "npm-run-all test:server test:client",
    "test:server": "echo \"Error: no test specified\" && exit 0",
    "test:client": "react-scripts test --env=jsdom",
    "//3": "=== DEV ===",
    "dev": "start yarn run dev:server && start yarn run dev:client",
    "dev:single": "concurrently \"yarn run dev:client\" \"yarn run dev:server\"",
    "dev:server": "cd ./_server && yarn run dev",
    "dev:client": "yarn run start:client",
    "//4": "=== BUILD ===",
    "build": "npm-run-all build-css build-js",
    "//5": "=== CRA SCRIPTS ===",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "cross-env PORT=5001 react-scripts start",
    "build-js": "react-scripts build",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://tnobrega@bitbucket.org/9beejava/react-web-bee.git"
  },
  "keywords": [
    "9bee",
    "react",
    "koa",
    "web"
  ],
  "author": "Tiago Nobrega",
  "license": "ISC",
  "homepage": "https://bitbucket.org/9beejava/react-web-bee#readme",
  "dependencies": {
    "axios": "0.17.1",
    "history": "4.7.2",
    "lodash": "4.17.4",
    "react": "16.2.0",
    "react-burger-menu": "2.2.3",
    "react-dom": "16.2.0",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.6",
    "react-scripts": "1.1.0",
    "redux": "3.7.2",
    "redux-actions": "2.2.1",
    "redux-saga": "0.16.0",
    "redux-thunk": "^2.2.0"
  }
}
